# .env.example
# Rename this file to .env and fill in your Spotify API credentials
# Get these from https://developer.spotify.com/dashboard/

SPOTIPY_CLIENT_ID='YOUR_SPOTIFY_CLIENT_ID'
SPOTIPY_CLIENT_SECRET='YOUR_SPOTIFY_CLIENT_SECRET'
# This must match one of the Redirect URIs set in your Spotify app settings
# Using 127.0.0.1:8000 for local development
SPOTIPY_REDIRECT_URI='http://127.0.0.1:8000/callback'
